{%- liquid
    assign sec_st = section.settings

    assign cnt_width = settings.cntwd | append: 'px'
    if sec_st.sw
        assign cnt_width = '100vw'
    endif

    assign is_rtl = false
    assign list_rtl = 'ar,dv,ha,he,ku,fa,ur,ug' | split: ','
    assign pm_left = 'left'
    assign pm_right = 'right'
    if list_rtl contains request.locale.iso_code or settings.rtl
        assign is_rtl = true
        assign pm_left = 'right'
        assign pm_right = 'left'
    endif
   
    assign logo = sec_st.logo
    assign istrnsparent = false
    assign page_type = request.page_type
    if page_type == 'index' and sec_st.trhead 
        assign istrnsparent = true
    elsif page_type == 'collection' and sec_st.trheadcl
        assign istrnsparent = true
    elsif page_type == 'blog' and sec_st.trheadbg
        assign istrnsparent = true
    elsif page_type == 'article' and sec_st.trheadar and article.image != blank
        assign istrnsparent = true
    endif
    if istrnsparent
        assign logo = sec_st.trLogo | default: logo
    endif   
-%}
{% capture site_header %}
{%- style -%}
     .hdr_Hamburger {
      --hdrHt:{{ sec_st.ht }}px;--hdrHtm:{{ sec_st.htm }}px;--hdrSp:{{ sec_st.head_space }}px;--logowd:{{ sec_st.logowd }}px;--logowdm:{{ sec_st.logowdm }}px;
    {%- if sec_st.bgimg != blank -%}
        background:{{ sec_st.bg }} url({{ sec_st.bgimg | image_url: width:sec_st.bgimg.width }}) {{ sec_st.bgrepeat }} {{ sec_st.bgposition | replace:"_", " " }}/{{ sec_st.bgsize }};
    {%- elsif sec_st.bgg != blank -%}
        background:{{ sec_st.bgg }};
    {%- else -%}
        background-color:{{ sec_st.bg }};
    {%- endif -%}
    {%- if sec_st.hdrbd -%}
        border-bottom:1px solid {{ sec_st.hdrbd_cl }};
    {%- endif -%}

    --cl:{{ sec_st.cl }}; --clh:{{ sec_st.clh }}; --icns:{{ sec_st.icon }}px; --icnsm:16px; --fs:{{ sec_st.fs }}px;
  }

  {%- if sec_st.cntlogo -%}
    .mbnavCol { margin-{{ pm_left }}:-5px; }
  {%- else -%}
    .navToggle { margin-{{ pm_right }}:-5px; }
  {%- endif -%}
  
  .searchinline .search .ad_inputs { max-width:250px;}
    
  @media only screen and (max-width:1024px){
    .main-search { order:3; width:100%; flex:auto; margin:5px 0 0; }
    {%- if sec_st.cntlogo -%}
      .icons-col { flex:1; }
    {%- endif -%}
  }
  
  {%- if section.settings.hdrStyle == '3' -%}
  #header .crlgTtl { font-size:{{ settings.navfs }}px; font-weight:{{ settings.navfw }};  {% if settings.nav_case %} text-transform: uppercase; {% endif %} font-family:var(--{{ settings.navft }}); }
  #header .crlgTtl { color:var(--cl); }
  #header .crlgTtl:after { background-color:var(--cl); }
  {%- endif -%}
  
  .icons-col .hdicon p {  {% if settings.nav_case %} text-transform: uppercase; {% endif %} font-family:var(--{{ settings.navft }}); font-weight:{{ settings.navfw }}; }
  {%- if section.settings.icntyp == '3' -%}
  .cartCount.pp {margin: -4px 0 0 4px;}  
  {%- endif -%}
      
      #header .crlgTtl,
      #header .social_icons a { color:var(--cl); }
      #header .crlgTtl:after { background-color:var(--cl); }
      .hdSearch { max-width:280px; }
      
  {%- if istrnsparent -%}
        .hdr_wrap:not(.sticky_hdr) { position:absolute; left:0; right:0; }
        .hdr_wrap.sticky_hdr, .hdr_wrap.header-hidden { position:fixed; }
        html:not(.actHmbNav, .searchact) .hdr_wrap:not(.sticky_hdr, :hover) { background-color:transparent; border-color:transparent; }
        html:not(.actHmbNav, .searchact) .hdr_wrap:not(.sticky_hdr, :hover) .hdicon,
        html:not(.actHmbNav, .searchact) .hdr_wrap:not(.sticky_hdr, :hover) .crlgTtl { color:{{ sec_st.thcl }}!important; }
        html:not(.actHmbNav, .searchact) .hdr_wrap:not(.sticky_hdr, :hover) .stlogo,
        :is(.hdr_wrap, .sticky_hdr):hover .trlogo, .sticky_hdr .trlogo,
        :is(.actHmbNav, .searchact) .hdr_wrap .trlogo { display:none !important; }
        .hdr_wrap .searchDrawer { top:var(--hdrHtm); }
        @media only screen and (min-width:1024px){
          .hdr_wrap .searchDrawer { top:var(--hdrHt); }
        }
        #PageContainer { --hdht:{{ sec_st.ht }}px;--hdhtm:{{ sec_st.htm }}px; }
    {%- endif -%}
  
  .leftcol .s_res {width:400px !important}

.hmbToggle:not(.active) .close, .hmbToggle.active .bar { display:none; }
  
.hmbMenu { height:0; overflow:hidden; z-index:667; {% if istrnsparent %}top:{{ sec_st.ht }}px;{% else %}top:100%;{% endif %}; border:0 none!important; }
.actHmbNav .hmbMenu { height:calc(100vh - var(--hdrHt, 80px)); overflow:auto; box-shadow:inset 0 0 15px #00000010; }
.hmbMenu #main_nav { padding:25px 0; width:250px; position: relative;}
.hmbMenu #main_nav > li { border-bottom:1px solid {{ settings.navcl | color_modify:'alpha', 0.10 }}; position: unset;; }
.hmbMenu #main_nav > li > a { justify-content:space-between; padding:15px 0; }
.hmbMenu #main_nav .mgmenu { left:260px; top:0; width:calc({{ cnt_width }} - 260px); background:none; box-shadow:none; padding:30px; transform: none; max-height: inherit;}
.hmbMenu #main_nav li:hover .mgmenu {left:250px; }
.hmbMenu #main_nav .mgmenu .page-width{ margin-left:0;}
.hmbMenu #main_nav .mgmenu ul > .gitem.lvl-1  {margin-bottom:25px;}

{%- unless sec_st.cntlogod -%}
@media only screen and (min-width:1024px){
  .mbnavCol { flex:inherit; }
}
{%- endunless -%}
{%- endstyle -%}

{%- capture logo -%}
<div class="header-logo">
  <a href="{{ routes.root_url }}" itemprop="url" class="hdr_logo_lnk" title="{{ shop.name }}">
    {%- assign logo_alt = logo.alt | default: shop.name | escape -%}
    {%- capture sizes -%}(max-width: {{ sec_st.logowd | times: 2 }}px) 50vw, {{ sec_st.logowd }}px{%- endcapture -%}
    {%- capture widths -%}{{ sec_st.logowd }}, {{ sec_st.logowd | times: 1.5 | round }}, {{ sec_st.logowd | times: 2 }}{% endcapture -%}
    {%- if istrnsparent and logo != blank -%}
        {%- assign trlogo_ht = sec_st.logowd | divided_by: logo.aspect_ratio -%}
        {{- logo | image_url: width: 600 | image_tag: class: 'imgAt trlogo db fw700', widths: widths, height: trlogo_ht, width: sec_st.logowd, alt: logo_alt, sizes: sizes, preload: true -}}
    {%- endif -%}
    {%- if sec_st.logo != blank -%}
        {%- assign logo_ht = sec_st.logowd | divided_by: sec_st.logo.aspect_ratio -%}
        {{- sec_st.logo | image_url: width: 600 | image_tag: class: 'imgAt stlogo db fw700', widths: widths, height: logo_ht, width: sec_st.logowd, alt: logo_alt, sizes: sizes, preload: true -}}
    {%- else -%}
        {{ shop.name }}
    {%- endif -%}
  </a>
</div>
{%- endcapture -%}

{%- capture promo -%}
    {%- if sec_st.promo_head != blank or sec_st.promo_text != blank -%}
      <div class="fl f-aic promo_txt hide-sm hide-md" style="--icns:{{ sec_st.prisz }}px">
        {%- if sec_st.prico != blank -%}
          {%- capture prico_width -%}{{ sec_st.prisz }}, {{ sec_st.prisz | times: 1.5 | round }}, {{ sec_st.prisz | times: 2 }}{%- endcapture -%}
          {{- sec_st.prico | image_url: width: 100 | image_tag: class: 'at-icon mr10', widths:prico_width, height:sec_st.prisz, width:sec_st.prisz, alt:sec_st.prico.alt -}}
        {%- endif -%}
        <div style="color:{{ sec_st.prcl }}; font-size:{{ sec_st.prfs }}px; line-height:1.1;">
          {%- if section.settings.promo_head != blank -%}<p style="color:{{ sec_st.prtcl }}; font-size:{{ sec_st.prtfs }}px; font-weight:{{ sec_st.prfw }};margin-bottom:2px;">{{ section.settings.promo_head }}</p>{%- endif -%}
          {%- if section.settings.promo_text != blank -%}<span>{{ section.settings.promo_text }}</span>{%- endif -%}
        </div>
      </div>
      {%- endif -%}
{%- endcapture -%}

{%- capture nav_toggle -%}
    <a href="#" class="hdicon hmbToggle mr30 hide-sm hide-md" role="button" title="{{ 'sections.navigation.menu' | t }}">
      <svg class="at-icon bar" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 464.205 464.205"><path d="m435.192 406.18h-406.179c-16.024 0-29.013-12.99-29.013-29.013s12.989-29.013 29.013-29.013h406.18c16.023 0 29.013 12.99 29.013 29.013-.001 16.023-12.99 29.013-29.014 29.013z"/><path d="m435.192 261.115h-406.179c-16.024 0-29.013-12.989-29.013-29.012s12.989-29.013 29.013-29.013h406.18c16.023 0 29.013 12.989 29.013 29.013s-12.99 29.012-29.014 29.012z"/><path d="m435.192 116.051h-406.179c-16.024 0-29.013-12.989-29.013-29.013s12.989-29.013 29.013-29.013h406.18c16.023 0 29.013 12.989 29.013 29.013s-12.99 29.013-29.014 29.013z"/></svg>
      <svg class="at-icon close"><use xlink:href="#icon-close"></use></svg>
      <p class="m0{% if sec_st.icntyp == '1' %} hide{% else %} hide-sm hide-md{% endif %}{% if sec_st.icntyp == '2' %} pdt5{% endif %}">{{ 'sections.navigation.menu' | t }}</p>
    </a>
    <a href="#" class="hdicon navToggle hide-lg" role="button" title="{{ 'sections.navigation.menu' | t }}"><svg class="at-icon" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 464.205 464.205"><path d="m435.192 406.18h-406.179c-16.024 0-29.013-12.99-29.013-29.013s12.989-29.013 29.013-29.013h406.18c16.023 0 29.013 12.99 29.013 29.013-.001 16.023-12.99 29.013-29.014 29.013z"/><path d="m435.192 261.115h-406.179c-16.024 0-29.013-12.989-29.013-29.012s12.989-29.013 29.013-29.013h406.18c16.023 0 29.013 12.989 29.013 29.013s-12.99 29.012-29.014 29.012z"/><path d="m435.192 116.051h-406.179c-16.024 0-29.013-12.989-29.013-29.013s12.989-29.013 29.013-29.013h406.18c16.023 0 29.013 12.989 29.013 29.013s-12.99 29.013-29.014 29.013z"/></svg></a>
{%- endcapture -%}
{%- capture extra_data -%}
    {%- if sec_st.leftcol == '1' -%}
        {{ promo }}
    {%- elsif sec_st.leftcol == '2' -%}
        <div class="fl1 leftcol fl f-aic gap hide-sm hide-md" style="--gap:15px;">
          {% if settings.mlcurrency or settings.mlLang %}{%- render 'currencyLang' -%}{% endif %}
        </div>
    {%- elsif sec_st.leftcol == '3' -%}
      <div class="searchinline hide-sm hide-md">{%- render 'search-form', frmClas: 'hdSearch' -%}</div>
    {%- endif -%}
{%- endcapture -%}
<header id="header" class="main_hdr fl f-aic col-gap pr{% if sec_st.sw %} fw-sec{% else %} page-width{% endif %}{% if istrnsparent %} trnsprt{% endif %}" data-sticky="{{ sec_st.sticky_type }}" data-section-id="header" style="{% if sec_st.trhead %}--thcl:{{ sec_st.thcl }};{% endif %}--gap:{{ sec_st.gap }}px;">
      <div class="mbnavCol fl f-aic fl1{% unless sec_st.cntlogo %} hide-sm hide-md{% endunless %}">
          {{ nav_toggle }}
          {% if settings.enable_search and sec_st.cntlogo %}
            <a class="hdicon searchIcn hide-lg" href="{{ routes.search_url }}" title="{{ 'sections.header.search' | t }}"><svg class="at-icon"><use xlink:href="#icon-search"></use></svg></a>
          {% endif %}
          {%- if sec_st.cntlogod -%}{{ extra_data }}{%- endif -%}
      </div>

      {{- logo -}}

      <div class="fl f-aic f-jce icons-col fl1">
        {%- unless sec_st.cntlogod -%}<div class="mr20 hide-sm hide-md">{{ extra_data }}</div>{% endunless %}
        {%- if settings.enable_search -%}
		  <a class="hdicon searchIcn{% if sec_st.cntlogo %} hide-sm hide-md{% endif %}{% if sec_st.leftcol == '3' %} hide-lg{% endif %}" href="{{ routes.search_url }}" title="{{ 'sections.header.search' | t }}">
              <svg class="at-icon{% if sec_st.icntyp == '3' %} hide-lg{% endif %}"><use xlink:href="#icon-search"></use></svg>
              <p class="m0{% if sec_st.icntyp == '1' %} hide{% else %} hide-sm hide-md{% endif %}{% if sec_st.icntyp == '2' %} pdt5{% endif %}">{{ 'sections.header.search' | t }}</p>
          </a>
        {%- endif -%}
        {%- if shop.customer_accounts_enabled -%}
		  <a class="hdicon user-menu" href="{%- if customer -%}{{ routes.account_url }}{%- else -%}{{ routes.account_login_url }}{%- endif -%}" title="{{ 'sections.header.account' | t }}">
                {%- render 'icon-account',  icntyp: sec_st.icntyp -%}
                <p class="m0{% if sec_st.icntyp == '1' %} hide{% else %} hide-sm hide-md{% endif %}{% if sec_st.icntyp == '2' %} pdt5{% endif %}">{{ 'sections.header.account' | t }}</p>
          </a>
        {%- endif -%}

        {%- if settings.wishlist -%}
          <a class="hdicon" href="{{ pages[settings.wishlist_page].url }}" title="{{ 'sections.header.wishlist' | t }}">
              <svg class="at-icon{% if sec_st.icntyp == '3' %} hide-lg{% endif %}"><use xlink:href="#icon-wishlist"></use></svg>
              {%- if sec_st.icntyp != '3' -%}<b id="wishlistItems" class="cartCount pp">0</b>{%- endif -%}
              <p class="m0{% if sec_st.icntyp == '1' %} hide{% else %} hide-sm hide-md{% endif %}{% if sec_st.icntyp == '2' %} pdt5{% endif %}">{{ 'sections.header.wishlist' | t }}</p>
          </a>
        {%- endif -%}

        {%- unless settings.catalog-mode -%}						
		<a href="{{ routes.cart_url }}" class="hdicon" id="cartLink" title="{{ 'sections.header.cart' | t }}">
            <svg class="at-icon{% if sec_st.icntyp == '3' %} hide-lg{% endif %}"><use xlink:href="#icon-cart"></use></svg>
            {%- if sec_st.icntyp != '3' -%}<b id="cart-icon-bubble" class="cartCount pp">{{ cart.item_count }}</b>{%- endif -%}
            <p class="m0{% if sec_st.icntyp == '1' %} hide{% else %} hide-sm hide-md{% endif %}{% if sec_st.icntyp == '2' %} pdt5{% endif %}">{{ 'sections.header.cart' | t }}{% if sec_st.icntyp == '3' %}<b id="cart-icon-bubble" class="cartCount pp">{{ cart.item_count }}</b>{% endif %}</p>
        </a>
        {%- endunless -%}
        {%- unless sec_st.cntlogo -%}
            <a href="#" class="hdicon navToggle hide-lg" title="{{ 'sections.navigation.menu' | t }}"><svg class="at-icon" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 464.205 464.205"><path d="m435.192 406.18h-406.179c-16.024 0-29.013-12.99-29.013-29.013s12.989-29.013 29.013-29.013h406.18c16.023 0 29.013 12.99 29.013 29.013-.001 16.023-12.99 29.013-29.014 29.013z"/><path d="m435.192 261.115h-406.179c-16.024 0-29.013-12.989-29.013-29.012s12.989-29.013 29.013-29.013h406.18c16.023 0 29.013 12.989 29.013 29.013s-12.99 29.012-29.014 29.012z"/><path d="m435.192 116.051h-406.179c-16.024 0-29.013-12.989-29.013-29.013s12.989-29.013 29.013-29.013h406.18c16.023 0 29.013 12.989 29.013 29.013s-12.99 29.013-29.014 29.013z"/></svg></a>
        {%- endunless -%}
      </div>
 </header>
    <nav id="main_navwr" class="hmbMenu anim pa w_100 hide-sm hide-md" role="navigation">
        <div class="pr h_100{% if sec_st.sw %} fw-sec{% else %} page-width{% endif %}">
            {%- render 'navigation', sec_st:sec_st, secblk:section.blocks, cnt_width:cnt_width, ishmb:true -%}
        </div>
    </nav>
    {%-liquid
        if settings.enable_search
            assign search_type = 'drawer'
            if sec_st.leftcol == '3'
                assign search_type = 'inline'
            endif
            render 'search-drawer', search_type:search_type, cnt_width:cnt_width, is_rtl:is_rtl
          endif
    -%}
<!--mmobile-->
{%- render 'mobile-nav', sec_st:sec_st, secblk:section.blocks -%}
{%- endcapture -%}
{{- site_header | replace:"<!--mmobile-->", "</div>" | strip_newlines | remove: "  " | remove: "	" -}}
{% schema %}
 {
    "name": "Header - Hamburger Menu",
	"limit": 1,
    "class": "hdr_wrap pr hdr_Hamburger",
    "enabled_on": {
        "groups": ["header"]
    },
    "settings": [      	       
      {
        "type": "range",
        "id": "ht",
        "min": 50,
        "max": 150,
        "step": 1,
        "label": "Header Height",
        "default": 70
      },
      {
        "type": "image_picker",
        "id": "logo",
        "label": "Logo Image"
      },
      {
        "type": "range",
        "id": "logowd",
        "min": 50,
        "max": 500,
        "step": 5,
        "label": "Logo Width",
        "default": 120
      },
      {
        "type": "checkbox",
        "id": "cntlogod",
        "label": "Logo Center?",
        "default": false
      },
      {
        "type": "select",
        "id": "icntyp",
        "label": "Addons",
        "default": "1",
        "options": [
          {
            "label": "Icons only",
            "value": "1"
          },         
          {
          "label": "Icon with text",
            "value": "2"
          },         
          {
          "label": "Text only",
            "value": "3"
          }
        ]
      },
      {
        "type": "range",
        "id": "icon",
        "min": 12,
        "max": 50,
        "step": 1,
        "label": "Icons Size",
        "unit": "px",
        "default": 18
      },
      {
        "type": "range",
        "id": "fs",
        "min": 10,
        "max": 20,
        "step": 1,
        "label": "Font Size",
        "unit": "px",
        "default": 12
      },           
      {
        "type": "color",
        "id": "cl",
        "label": "Icons & Text",
        "default": "#555"
      },
      {
        "type": "color",
        "id": "clh",
        "label": "Icons & Text Hover",
        "default": "#000"
      },      
      {
            "type": "select",
            "id": "leftcol",
            "label": "Additional Option",
            "default": "3",
            "info":"Enable Currency & Language from Theme Settings",
            "options": [
              {
                "label": "Promotional Text",
                "value": "1"
              },         
              {
                "label": "Currency & Language",
                "value": "2"
              },         
              {
                "label": "Search Box",
                "value": "3"
              }
            ]
      },
	  {
        "type": "header",
        "content": "Promotional Text ====",
        
      },	 
      {
          "type": "image_picker",
          "id": "prico",
          "label": "Image or Svg Icon",
          "info":"[Available SVG icons.](https://adornthemes.com/documentation/vogal/svg-icons.html)",
           
      },
      {
        "type": "range",
        "id": "prisz",
        "label": "Icon Size",
        "default": 30,
        "min": 10,
        "max": 100,
        "step": 1,
        "unit": "px",
        
      },
      {
        "type": "text",
        "id": "promo_head",
        "label": "Heading",
        "default": "EXPERT SUPPORT",
        
      },
      {
        "type": "color",
        "id": "prtcl",
        "label": "Color",
        "default": "#111",
        
      },
      {
        "type": "range",
        "id": "prtfs",
        "label": "Font Size",
        "default": 16,
        "min": 10,
        "max": 60,
        "step": 1,
        "unit": "px",
        
      },
      {
        "type": "select",
        "id": "prfw",
        "label": "Font Weight",
        "default": "500",
        "options": [
          {
            "value": "300",
            "label": "Light"
          },
          {
            "value": "400",
            "label": "Regular"
          },
          {
            "value": "500",
            "label": "Medium"
          },
          {
            "value": "600",
            "label": "Semi Bold"
          },
          {
            "value": "700",
            "label": "Bold"
          }
        ],
        
      },
      {
        "type": "text",
        "id": "promo_text",
        "label": "Text",
        "default": "Available 24\/7 via chat",
        
      },
	  {
        "type": "color",
        "id": "prcl",
        "label": "Color",
        "default": "#333",
        
      },
      {
        "type": "range",
        "id": "prfs",
        "label": "Font Size",
        "default": 12,
        "min": 10,
        "max": 30,
        "step": 1,
        "unit": "px",
        
      },      	  
      {
        "type": "header",
        "content": "Sticky Header ====="
      },
      {
        "type": "select",
        "id": "sticky_type",
        "label": "Sticky header",
        "default": "top",
        "options": [
          {
            "value": "none",
            "label": "None"
          },
          {
            "value": "always",
            "label": "Always"
          },
          {
            "value": "top",
            "label": "On scroll up"
          }
        ]
      },
      {
        "type": "checkbox",
        "id": "sticky_shadow",
        "label": "Show Bottom Shadow?",
        "default": true
      },
	  {
        "type": "header",
        "content": "Transparent Header ====="
      },
      {
        "type": "checkbox",
        "id": "trhead",
        "label": "Enable for home page",
        "default": false
      },
      {
        "type": "checkbox",
        "id": "trheadcl",
        "label": "Enable for collection page",
        "default": false
      },
      {
        "type": "checkbox",
        "id": "trheadbg",
        "label": "Enable for blog page",
        "default": false
      },
      {
        "type": "checkbox",
        "id": "trheadar",
        "label": "Enable for article page",
        "default": false
      },
	  {
        "type": "image_picker",
        "id": "trLogo",
        "label": "Logo Image",
        
      },      
	  {
        "type": "color",
        "id": "thcl",
        "label": "Text & Icon",
        "default": "#fff",
        
      },
	  {
        "type": "header",
