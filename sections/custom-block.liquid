{%- capture custom_content -%}
{%- liquid 
    assign section_st = section.settings
    assign page_width = settings.cntwd | append: 'px'
    if section_st.sw
        assign page_width = '100vw'
    endif
-%}
<div class="sec_cov{% if section_st.hidem %} hide-sm{% endif %}{% if section_st.hided %} hide-md hide-lg{% endif %}{% if section_st.bgimg != blank %} bgImg lazyload zi1{% endif %}" style="--sbg:{% if section_st.bgg != blank %}{{ section_st.bgg }}{% else %}{{ section_st.bg }}{% endif %};--sst:{{ section_st.sst }}px;--ssb:{{ section_st.ssb }}px;--sstm:{{ section_st.sstm }}px;--ssbm:{{ section_st.ssbm }}px;--overlay:{{ section_st.overlay | color_modify: 'alpha', section_st.overlay_op }};"  
  {% if section_st.bgimg != blank %}data-bgset="{{ section_st.bgimg | image_url: width: section_st.bgimg.width | image_tag: widths: '400,600,800,1000,1400,1600,2000,2400,3000' | split: 'srcset="' | last | split: '"' | first }}" data-sizes="auto" data-parent-fit="contain"{% endif %}>
    <div class="{% if section_st.sw %}fw-sec {% else %}page-width{% endif %} custom-block {{section_st.style }}">  
        {% render 'sec-title' %}	
        <div class="grid custom-cnt gspace" style="--grsp:{{ section_st.gspace | divided_by: 2.0 }}px;--grspm:{{ section_st.gspacem | divided_by: 2.0 }}px;">
                {% for block in section.blocks %}
                    {% assign blk_st = block.settings %}
                    <div class="gitem ct_item cti{{ block.id }} {{ blk_st.width }}-md {{ blk_st.alignment }}{%  if block.type == "richtxt" %} customrich{% endif %}{% if blk_st.hidem %} hide-sm{% endif %} {% render 'animate-data', css:true %}" {% render 'animate-data' %} {{ block.shopify_attributes }}>
                        <div class="of_hidden ct_item_in ct_item_{{ block.type }} w_100{% if block.type == 'image'%} tc{% endif %}" style="--bg:{{ blk_st.clbg }};--brad:{{ blk_st.brad }}px;">                           
                            {%- capture sizes -%}
                                {%- if blk_st.width == 'wd20' -%}
                                    (min-width:1024px) calc(({{ page_width }} - {{ section_st.gspace | times:4 }}px) / 5),(min-width:768px) calc((100vw - {{ section_st.gspace | times:4 }}px) / 5), calc(100vw - 15px)
                                {%- elsif blk_st.width == 'wd25' -%}
                                    (min-width:1024px) calc(({{ page_width }} - {{ section_st.gspace | times:3 }}px) / 4),(min-width:768px) calc((100vw - {{ section_st.gspace | times:3 }}px) / 4), calc(100vw - 15px)
                                {%- elsif blk_st.width == 'wd33' -%}
                                    (min-width:1024px) calc(({{ page_width }} - {{ section_st.gspace | times:2 }}px) / 3),(min-width:768px) calc((100vw - {{ section_st.gspace | times:2 }}px) / 3), calc(100vw - 15px)
                                {%- elsif blk_st.width == 'wd50' -%}
                                  (min-width:1024px) calc(({{ page_width }} - {{ section_st.gspace }}px) / 2),(min-width:768px) calc((100vw - {{ section_st.gspace }}px) / 2), calc(100vw - 15px)
                                {%- elsif blk_st.width == 'wd66' -%}
                                  (min-width:1024px) calc(({{ page_width }} - {{ section_st.gspace }}px) / 3 * 2),(min-width:768px) calc(({{ page_width }} - {{ section_st.gspace }}px) / 3 * 2), calc(100vw - 15px)
                                {%- elsif blk_st.width == 'wd75' -%}
                                  (min-width:1024px) calc(({{ page_width }} - {{ section_st.gspace }}px) / 5 * 3),(min-width:768px) calc(({{ page_width }} - {{ section_st.gspace }}px) / 5 * 3), calc(100vw - 15px)
                                {%- elsif blk_st.width == 'w_100' -%}
                                  (min-width:1024px) {{ page_width }}, 100vw
                                {%- endif -%}
                            {%- endcapture -%}
                        {% case block.type %}
                          
                            {% when 'banner' %}
                             <div class="pr brad of_hidden" style="--cl:{{ blk_st.cl }};--fs:{{ blk_st.fs }}px;--fsm:{{ blk_st.fsm }}px;--fw:{{ blk_st.fw }};--lh:{{blk_st.lh }};--rounded:{{ blk_st.brad }}px;">                                  									 
                                     <picture class="overlay" style="--overlay:{{ blk_st.overlay | color_modify: 'alpha', blk_st.overlay_op }};">
                                    {% if blk_st.image != blank  %}
                                        {% if blk_st.btnurl != blank %}<a href="{{ blk_st.btnurl }}"  class="pr db">{% endif %}
                                        {{ blk_st.image | image_url: width: blk_st.image.width | image_tag: class:'imgAt', width:blk_st.image.width, height:blk_st.image.height, sizes:sizes, widths:"200,400,600,800,1000,1200,1400,1600,2000" }}
                                        {% if blk_st.btnurl != blank %}</a>{% endif %}
                                      {% else %}
                                        <p> {{ 'collection-apparel-3' | placeholder_svg_tag: 'placeholder-svg' }}</p>
                                    {% endif %}
                                     </picture>
                          
                                    <div class="fl f-col f-aic {{ blk_st.alignment }} tc pa all0 pd30 zi1">
                                        {% if blk_st.title != blank %}<h3 class="rgTxt {{ blk_st.ff }}" style=" margin-bottom:{{ blk_st.tbm }}px;">{{ blk_st.title }}</h3>{% endif %}                                       
                                        {% if blk_st.text != blank %}<div class="rte mb20" style="color:{{ blk_st.cl }};">{{ blk_st.text }}</div>{% endif %}
                                         {% if blk_st.date != blank %}<countdown-time class="atCounter box_timer fl ttu f-jcc mb25" data-date="{{ blk_st.date }}" style="--fs:{{ blk_st.cd_fs }}px;--fsl:{{ blk_st.cd_fsl }}px;--fw:{{ blk_st.cd_fw}};" {{ block.shopify_attributes }}>
                                        <span><span class="tm days">00</span>{{ 'date_formats.days' | t }}</span> <span class="fl f-jcc f-aic">:</span>
                                        <span><span class="tm hours">00</span>{{ 'date_formats.hours' | t }}</span> <span class="fl f-jcc f-aic">:</span>
                                        <span><span class="tm minutes">00</span>{{ 'date_formats.minuts' | t }}</span> <span class="fl f-jcc f-aic">:</span>
                                        <span><span class="tm seconds">00</span>{{ 'date_formats.seconds' | t }}</span> 
                                        </countdown-time>  {% endif %}
                                        {% if blk_st.btntxt != blank and blk_st.btnurl != blank %}
                                            <a class="btn mb20 {{ blk_st.btn-size }} {{ blk_st.btn-style }}" href="{{ blk_st.btnurl }}">{{ blk_st.btntxt }}</a>
                                        {% endif %}
                                    </div>                                            
                              </div>                                               
                             
                              {% when 'richtxt' %}															
                                <div class="tc w_100" style="--cl:{{ blk_st.cl }};--fs:{{ blk_st.fs }}px;--fsm:{{ blk_st.fsm }}px;--fw:{{ blk_st.fw }};--lh:{{blk_st.lh }}">                               
                                    <div class="custom-text">
                                        {% if blk_st.title != blank %}<h3 class="rgTxt {{ blk_st.ff }}" style=" margin-bottom:{{ blk_st.tbm }}px;"><a href="{{ blk_st.btnurl }}" class="tdn">{{ blk_st.title }}</a></h3>{% endif %}
                                        {% if blk_st.text != blank %}<div class="rte mb20" style="color:{{ blk_st.cl }};">{{ blk_st.text }}</div>{% endif %}
                                        {% if blk_st.btntxt != blank and blk_st.btnurl != blank %}
                                            <a class="btn {{ blk_st.btn-size }} {{ blk_st.btn-style }}" href="{{ blk_st.btnurl }}">{{ blk_st.btntxt }}</a>
                                        {% endif %}
                                    </div>
                                    <style>
                                    {%  if blk_st.txtpad != blank %}
                                          .cti{{ block.id }} .ct_item_in { padding:{{blk_st.txtpad }}; }
                                    {% endif %}
                                   {%  if blk_st.txtpadt != blank %}
                                      @media only screen and (max-width:1200px){
                                          .cti{{ block.id }} .ct_item_in { padding:{{blk_st.txtpadt }}; }
                                      }
                                    {% endif %}
                                    {%  if blk_st.txtpadm != blank %}
                                          @media only screen and (max-width:766px){
                                              .cti{{ block.id }} .ct_item_in { padding:{{blk_st.txtpadm }}; }
                                          }                                  
                                    {% endif %}
                                    </style>             
                                </div>
                          
                            {% when 'text' %}								
							
                                <div class="{{ blk_st.align_text }}" style="--cl:{{ blk_st.cl }};--fs:{{ blk_st.fs }}px;--fsm:{{ blk_st.fsm }}px;--fw:{{ blk_st.fw }};--lh:{{blk_st.lh }}">
                                    {% if blk_st.dis_img %}
									  {% if blk_st.image != blank  %}
                                        {% if blk_st.btnurl != blank %}<a href="{{ blk_st.btnurl }}">{% endif %}
                                            {{ blk_st.image | image_url: width: blk_st.image.width | image_tag: class:'ctimg mb20 h_auto', width:blk_st.image.width, height:blk_st.image.height, sizes:sizes, widths:"200,400,600,800,1000,1200,1400,1600,2000" }}
                                        {% if blk_st.btnurl != blank %}</a>{% endif %}
									  {% else %}
										 <p>{% capture current %}{% cycle 1, 2, 3 %}{% endcapture %}{{ 'collection-apparel-' | append: current | placeholder_svg_tag: 'placeholder-svg' }}</p>
									  {% endif %}
                                    {% endif %}
                                    <div class="custom-text">
                                        {% if blk_st.title != blank %}<h3 class="rgTxt {{ blk_st.ff }}" style=" margin-bottom:{{ blk_st.tbm }}px;">{{ blk_st.title }}</h3>{% endif %}                                       
                                        {% if blk_st.text != blank %}<div class="rte mb20" style="color:{{ blk_st.cl }};">{{ blk_st.text }}</div>{% endif %}
                                         {% if blk_st.date != blank %}<countdown-time class="atCounter box_timer fl ttu {{ blk_st.align_text}} mb25" data-date="{{ blk_st.date }}" style="--fs:{{ blk_st.cd_fs }}px;--fsl:{{ blk_st.cd_fsl }}px;--fw:{{ blk_st.cd_fw}};" {{ block.shopify_attributes }}>
                                        <span><span class="tm days">00</span>{{ 'date_formats.days' | t }}</span> <span class="fl f-jcc f-aic">:</span>
                                        <span><span class="tm hours">00</span>{{ 'date_formats.hours' | t }}</span> <span class="fl f-jcc f-aic">:</span>
                                        <span><span class="tm minutes">00</span>{{ 'date_formats.minuts' | t }}</span> <span class="fl f-jcc f-aic">:</span>
                                        <span><span class="tm seconds">00</span>{{ 'date_formats.seconds' | t }}</span> 
                                        </countdown-time>  {% endif %}
                                        {% if blk_st.btntxt != blank and blk_st.btnurl != blank %}
                                            <a class="btn mb20 {{ blk_st.btn-size }} {{ blk_st.btn-style }}" href="{{ blk_st.btnurl }}">{{ blk_st.btntxt }}</a>
                                        {% endif %}
                                    </div>
                                    <style>                                       
                                        {% if blk_st.bg %}    										      
										 	.cti{{ block.id }} { align-self:stretch; display:flex;} 
                                            .cti{{ block.id }} .ct_item_in {display: flex; align-items: center; padding:35px;  background:{{ blk_st.clbg }}; border-radius:{{blk_st.boxrad}}px;}         
											.cti{{ block.id }}.f-ass .ct_item_in {align-items: start; }
               								.cti{{ block.id }}.f-ase .ct_item_in {align-items: end;}  
										    .cti{{ block.id }} .ct_item_in > div { width:100%;} 
                                        {% endif %}              	
                                    </style>             
                                </div>                                                  
                          
                              {% when 'video' %}
                                <div class="video_bnr pr of_hidden brad" style="--rounded:{{ blk_st.vbr }}px;">
                                    {% capture video_data %}
                                      {%- liquid
                                          assign poster = blk_st.poster | default: blk_st.video.preview_image
                                          assign poster_size = poster | image_url: width: poster.width
                                          assign psratio = '56.25'
                                          if poster
                                             assign psratio = 1 | divided_by: poster.aspect_ratio | times: 100
                                          endif
                                          assign muted = blk_st.muted
                                          assign loop = false
                                          if blk_st.autoplay == true
                                             assign muted = true
                                             assign loop = true
                                          endif 										
                                      -%}
                                      <div class="vd-wrap" style="--ratio:{{ psratio }}%;">
                                        {% # theme-check-disable RemoteAsset,DeprecateBgsizes %}
                                        {% if blk_st.video_url == blank and blk_st.video == blank %}
                                          <iframe class="db" src="//www.youtube.com/embed/_9VUPq3SxOc?rel=0&showinfo=0&vq=720" width="850" height="480" frameborder="0" allowfullscreen></iframe>
                                        {% elsif blk_st.video_url != blank %}
                                          {% if blk_st.video_url.type == "youtube" %}
                                              <iframe class="db lazyload" data-src="//www.youtube.com/embed/{{ blk_st.video_url.id }}?controls=0&autoplay=1&rel=0&showinfo=0{% if blk_st.video_style == "bg" %}&mute=1{% endif %}" width="850" height="480" frameborder="0" allow="autoplay" allowfullscreen></iframe>
                                          {% endif %}
                                          {% if blk_st.video_url.type == "vimeo" %}
                                              <iframe class="db lazyload" data-src="//player.vimeo.com/video/{{ blk_st.video_url.id }}?background=1&byline=0&portrait=0&badge=0" width="850" height="480" frameborder="0" allow="autoplay; fullscreen" allowfullscreen></iframe>
                                          {% endif %}
                                        {% elsif blk_st.video != blank %}
                                            {{ blk_st.video | video_tag: image_size:poster_size, loop:loop, autoplay:blk_st.autoplay, muted:muted, controls:blk_st.controls, poster:poster_size, class:'w_100 db' }} 
                                        {% endif %}
                                        {% # theme-check-enable DeprecateBgsizes,DeprecateBgsizes %}
                                      </div>
                                    {% endcapture %}

                                    {% if blk_st.video_style == "pp" %}
                                      <a href="{{ blk_st.video_url }}?controls=0&autoplay=1&rel=0&showinfo=0" class="video_pop popup-link" data-popup="vd{{ block.id }}" aria-haspopup="dialog">
                                         <div class="vd-wrap" style="--ratio:{{ 1 | divided_by: poster.aspect_ratio | times: 100 }}%;--icns:50px;--cl:{{ blk_st.cl }};">
                                            {% if poster %}
                                                {{ poster | image_url: width: poster.width | image_tag: class:'imgFt ctimg', sizes:sizes, widths:"200,400,600,800,1000,1200,1400,1600,2000" }}
                                            {% else %}
                                                <p class="db pd30">Please upload video poster image.</p>
                                            {% endif %}
                                         </div>
                                    {% endif %}
                                        {% if blk_st.video_style == "bg" or blk_st.video_style == "df" %}{{ video_data }}{% endif %}
                                         <div class="fl f-jcc f-aie {{ blk_st.text_align }} vdtWrap{% if blk_st.video_style == "df" %}1{% endif %}" style="--icns:50px;--cl:{{ blk_st.cl }};--fs:{{ blk_st.fs }}px;--fsm:{{ blk_st.fsm }}px;--fw:{{ blk_st.fw }};--lh:{{blk_st.lh }};">
                                             <div class="video_txt pd20{% if blk_st.talign %} tl{% else %} tc{% endif %}">
                                                 {%- if blk_st.video_style == "pp" -%}<svg class="at-icon playBtn mb15" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 512 512"><path d="M256 504c137 0 248-111 248-248S393 8 256 8 8 119 8 256s111 248 248 248zM40 256c0-118.7 96.1-216 216-216 118.7 0 216 96.1 216 216 0 118.7-96.1 216-216 216-118.7 0-216-96.1-216-216zm331.7-18l-176-107c-15.8-8.8-35.7 2.5-35.7 21v208c0 18.4 19.8 29.8 35.7 21l176-101c16.4-9.1 16.4-32.8 0-42zM192 335.8V176.9c0-4.7 5.1-7.6 9.1-5.1l134.5 81.7c3.9 2.4 3.8 8.1-.1 10.3L201 341c-4 2.3-9-.6-9-5.2z"/></svg>{%- endif -%}
                                                 {%- if blk_st.title != blank -%}<div class="rgTxt {{ blk_st.ff }}{% if blk_st.hide_text %} hide-sm{% endif %}">{{ blk_st.title }}</div>{%- endif -%}
                                                 {%- if blk_st.video_style == "bg" or blk_st.video_style == "df" -%}
                                                    {%- if blk_st.btntxt != blank -%}
                                                     <a href="{{ blk_st.btnurl }}" class="mt15 btn {{ blk_st.btn-style }}{% if blk_st.hide_btn %} hide-sm{% endif %}">{{ blk_st.btntxt }}</a>
                                                     {%- endif -%}
                                                  {%- endif -%}
                                              </div>
                                         </div>
                                    {% if blk_st.video_style == "pp" %}
                                      </a>
                                      <div id="vd{{ block.id }}" class="tinglebox pp-hide vd">{{ video_data | replace: "vd-wrap","vd-wrap1" }}</div>
                                    {% endif %}
                                </div>
                          
                            {% when 'product' %}
                                {%- assign product = all_products[blk_st.product] -%}
                                {% if product.title.size > 0 %}
                                    {% render 'product-card-grid', product: product, block: block.settings, section: section.settings, sizes:sizes %}
                                {% else %}
                                    <div class="grid_bx">
                                        <a href="#" class="grid_lnk">
                                            <div class="grid_img_wr">{% capture current %}{% cycle 1, 2, 3, 4 %}{% endcapture %}{{ 'product-apparel-' | append: current | placeholder_svg_tag: 'placeholder-svg' }}</div><br/>
                                            <div class="h4 grid_title">{{ 'homepage.onboarding.product_title' | t }}</div>
                                            <div class="grid_meta">{% render 'product-price' %}</div>
                                        </a>
                                    </div>
                                {% endif %}
						
                            {% when 'collection' %}
                             {%- assign collection = collections[blk_st.collection] -%}
                             {%  assign collection_image = blk_st.image | default:collection.image | default: collection.products.first.featured_image %}
                              <div class="pr of_hidden{% if section_st.zoom %} zoom_hov{% endif%}">
                                  <a href="{{ blk_st.url | default:collection.url }}" title="{{ blk_st.title }}">     
                                    <div class="img pr of_hidden">
                                      {%- if collection_image != blank -%}{{ collection_image | image_url: width: collection_image.width | image_tag: loading:'lazy', width:collection_image.width, height:collection_image.height, class:'imgFl', sizes:sizes, widths:"180,320,400,560,740,900,1200" }}
                                      {%- else -%}{% capture current %}{% cycle 1, 2, 3, 4 %}{% endcapture %}{{ 'collection-apparel-' | append: current | placeholder_svg_tag: 'placeholder-svg' }}{%- endif -%}
                                    </div>          
                                    {%  if blk_st.title != blank %}<div class="btn {{ blk_st.btn-size}} {{ blk_st.btn-style }}">{{ blk_st.title }}</div> {% endif %}
                                  </a>
                              </div>
                                                        
                            {% when 'html' %}
                               <div class="rte"> {{ blk_st.code }}</div>                          
                            {% endcase %}
                        </div>
                    </div>
                {% endfor %}
            </div>
            {% if section.blocks.size == 0 %}{% render 'no-blocks' %}{% endif %}
        </div>  
</div>
{%- endcapture -%}
{{ custom_content | strip_newlines | remove: "	" }}
{% schema %}
{
    "name": "Custom Block",
    "class": "index-section",
    "disabled_on": {
    "groups": ["header", "footer", "custom.overlay"]
    },
    "settings": [	 
        {
            "type": "text",
            "id": "title",
            "label": "Heading",
            "default": "Custom content"         
        },
        {
            "type": "textarea",
            "id": "subtitle",
            "label": "Sub Heading",
            "default": "Describe your products, collection, content etc..."
        }, 
      {
		 "type": "header",
		 "content":"Grid ==="
     },			 
	{
        "type": "range",
        "id": "gspace",
        "label": "Grid gap",
        "default": 30,
        "min": 0,
        "max": 100,
        "step": 1,
		"unit":"px"
		
      },
     {
        "type": "range",
        "id": "gspacem",
        "label": "Grid gap (mobile)",
        "default": 15,
        "min": 0,
        "max": 15,
        "step": 1,
		"unit":"px"		
      },
       {
            "type": "select",
            "id": "prbox_style",
            "label": "Grid style",
            "default": "grid_1",
            "options": [
                    {
                      "value": "grid_1",
                      "label": "Icons - Bottom Center"
                    },
                    {
                      "value": "grid_2",
                      "label": "Info. Hover Image"
                    },
                    {
                      "value": "grid_3",
                      "label": "Icons - Bottom Left"
                    },         
                    {
                      "value": "grid_4",
                      "label": "Icons - Top Right"
                    }                   
                ]            
        },
	   {
            "type": "header",
            "content": "Section Design ==="
        },
       {
            "type":"checkbox",
            "id":"sw",
            "label":"Fullwidth?",
            "default": false           
        },	       
        {
            "type": "color",
            "id": "bg",
            "label": "Background",
            "default":"#fff"	
        },
        {
            "type": "color_background",
            "id": "bgg",
            "label": "Gradient background"
        },	
        {
            "type": "image_picker",
            "id": "bgimg",
            "label": "Background image"
          }, 
        {
            "type": "color",
            "id": "overlay",
            "label": "Image overlay",
            "default": "#111"
        },
        {
            "type": "range",
            "id": "overlay_op",
            "min": 0,
            "max": 1,
            "step": 0.1,
            "label": "Overlay opacity",
            "unit": "%",
            "default": 0.5
        },
       {
            "type": "color",
            "id": "headings_cl",
            "label": "Text"
        },
		{
			"type": "range",
			"id": "sst",
			"min": 0,
			"max": 100,
			"step": 5,
			"label": "Spacing top",
			"unit": "px",
			"default":40
        },
		{
			"type": "range",
			"id": "ssb",
			"min": 0,
			"max": 100,
			"step": 5,
			"label": "Spacing bottom",
			"unit": "px",
			"default":40
        },		
		{
			"type": "range",
			"id": "sstm",
			"min": 0,
			"max": 100,
			"step": 5,
			"label": "Spacing top (mobile)",
			"unit": "px",
			"default":15
        },
		{
			"type": "range",
			"id": "ssbm",
			"min": 0,
			"max": 100,
			"step": 5,
			"label": "Spacing bottom (mobile)",
			"unit": "px",
			"default":15
        },
         {
            "type": "checkbox",
            "id": "hidem",
            "label": "Hide on mobile?",
            "default": false
        },
        {
            "type": "checkbox",
            "id": "hided",
            "label": "Hide on desktop?",
            "default": false
        }
    ],
    "blocks": [
      {
            "type": "banner",
            "name": "Banner",
            "settings": [
                 {
                    "type": "checkbox",
                    "id": "hidem",
                    "label": "Hide in mobile?",
                    "default": false			
                },	                                 
                {
                    "type": "image_picker",
                    "id": "image",
                    "label": "Image"
                },
                 {
                    "type": "color",
                    "id": "overlay",
                    "label": "Image overlay",
                    "default": "#111"
                },
                {
                    "type": "range",
                    "id": "overlay_op",
                    "min": 0,
                    "max": 1,
                    "step": 0.1,
                    "label": "Overlay opacity",
                    "unit": "%",
                    "default": 0.5
