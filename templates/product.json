label{font-weight:600;font-size:15px;color:#2b2b2b;margin-bottom:8px}\n.cl-ps__pills{display:flex;flex-wrap:wrap;gap:var(--ps-gap)}\n.cl-ps__btn{appearance:none;border:1.25px solid var(--ps-green);background:#fff;color:var(--ps-green);border-radius:var(--ps-radius);padding:var(--ps-pad-y) var(--ps-pad-x);font-size:var(--ps-fs);line-height:1.2;cursor:pointer;transition:background .18s,color .18s,border-color .18s,box-shadow .18s}\n.cl-ps__btn:hover{filter:brightness(.98)}\n.cl-ps__btn.is-active{background:var(--ps-dark);color:#fff;border-color:var(--ps-dark);box-shadow:0 6px 14px rgba(0,0,0,.08)}\n@media(max-width:480px){:root{--ps-fs:15px;--ps-pad-y:9px;--ps-pad-x:14px}}\n<\/style>\n\n<div class=\"cl-ps\" data-option-name=\"Print Size\" role=\"group\" aria-label=\"Print Size\">\n  <div class=\"cl-ps__label\">Print Size<\/div>\n  <div class=\"cl-ps__pills\">\n    <button type=\"button\" class=\"cl-ps__btn\" data-value=\"Album Size (A4)\">A4<\/button>\n    <button type=\"button\" class=\"cl-ps__btn\" data-value='30 x 40cm \/ 12 x 16\"'>12 x 16\" (Inches)<\/button>\n    <button type=\"button\" class=\"cl-ps__btn\" data-value=' 18 x 24\"'>18 x 24\" (Inches)<\/button>\n  <\/div>\n<\/div>\n\n<script>\n(function(){\n  function findForm(root){\n    return root.closest('product-form')?.querySelector('form')\n        || document.querySelector('product-form form')\n        || document.querySelector('form[action*=\"\/cart\/add\"]')\n        || document.querySelector('form[id*=\"product-form\"]');\n  }\n  function setOption(form,name,val){\n    var n='options['+name+']';\n    var sel=form.querySelector('select[name=\"'+n+'\"]');\n    if(sel){ sel.value=val; sel.dispatchEvent(new Event('change',{bubbles:true})); return; }\n    var radios=[...form.querySelectorAll('input[type=\"radio\"][name=\"'+n+'\"]')];\n    var r=radios.find(x=>x.value.trim().toLowerCase()==val.trim().toLowerCase());\n    if(r && !r.checked) r.click();\n  }\n  function getOption(form,name){\n    var n='options['+name+']';\n    var s=form.querySelector('select[name=\"'+n+'\"]');\n    if(s) return s.value;\n    var r=form.querySelector('input[type=\"radio\"][name=\"'+n+'\"]:checked');\n    return r?r.value:null;\n  }\n  document.querySelectorAll('.cl-ps').forEach(group=>{\n    var opt=(group.getAttribute('data-option-name')||'Print Size').trim();\n    var form=findForm(group);\n    if(!form) return;\n    var pills=[...group.querySelectorAll('.cl-ps__btn')];\n    var cur=getOption(form,opt);\n    if(cur){\n      pills.forEach(b=>b.classList.toggle('is-active', b.dataset.value.toLowerCase()==cur.toLowerCase()));\n    }else{\n      pills[0].classList.add('is-active'); setOption(form,opt,pills[0].dataset.value);\n    }\n    pills.forEach(btn=>{\n      btn.addEventListener('click',()=>{\n        pills.forEach(x=>x.classList.remove('is-active'));\n        btn.classList.add('is-active');\n        setOption(form,opt,btn.dataset.value);\n      });\n    });\n    form.addEventListener('change',e=>{\n      if(!e.target.name||!e.target.name.includes('options['+opt+']'))return;\n      var v=getOption(form,opt)||'';\n      pills.forEach(x=>x.classList.toggle('is-active', x.dataset.value.toLowerCase()==v.toLowerCase()));\n    });\n  });\n})();\n<\/script>","bm":15}},"ccd4e61d-1477-4664-bd33-351a9aa1ec00":{"type":"inventory","settings":{"qty_sel":true,"qlbl":true,"inv":true,"maxqty":14,"fs":13,"fcase":false,"font-style":"italic","instock":"#007133","lstock":"#fda43c","ostock":"#dd2f45","anim":true,"bm":15}},"f3aed949-c526-4cd5-affb-f209a0499d89":{"type":"cta","settings":{"gift_recipient":false,"atc-style":"btn1","wishlist-btn":"btn2","ext-btn":"btn6","dynamic_btn":false,"btnbg":"#f4857b","btntext":"#ffffff","btnbdr":"#f4857b","bis":true,"bg":"#fafafa","bd":"#eeeeee","bdr":5,"bm":20}},"addons_3DKyxX":{"type":"addons","settings":{"askpr":true,"isdr":true,"page":"returns-policy","share":true,"cl":"#333333","clh":"#40b884","fs":13,"fw":"500","bm":15}},"hr_zDHmMw":{"type":"hr","settings":{"hrc"